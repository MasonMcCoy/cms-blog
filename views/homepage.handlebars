{{#each blogPosts as |blogPost| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h3>
        <a href="/blogpost/{{blogPost.id}}">{{blogPost.title}}</a>
    </h3>
    <span>Created by {{blogPost.user.username}} on {{blogPost.date_created}}</span>
  </div>
  <div class="col-md-7">
    <p>
      {{blogPost.contents}}
    </p>
  </div>
</div>
{{/each}}